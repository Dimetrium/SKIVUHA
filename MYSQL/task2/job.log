CREATE TABLE task2 (num INT, var VARCHAR(255), text TEXT);


DELIMITER //  
CREATE PROCEDURE test ()  
BEGIN 
DECLARE count INT DEFAULT 1;
WHILE count <= 700000 DO
INSERT INTO task2 (num, var, text) VALUE (count, md5(RAND()), md5(RAND()));
SET count = count + 1;
END WHILE;
END//
DELIMITER ;
	
call test ();

